  "version": "1.0.0",
  "description": "web service custospet",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "build": "tsc -p tsconfig.json && npx sequelize-cli db:migrate",
    "start": "node dist/index.js",
